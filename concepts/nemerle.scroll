   // 'xml' - macro from Nemerle.Xml.Macro library which alows to inline XML literals into the nemerle-code
   def html = xml <#
     <html>
       <head>
         <title>$title</title>
       </head>
       <body>
         <ul $when(authors.Any())>
           <li $foreach(author in authors)>$author</li>
