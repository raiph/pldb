codeWithHeader Example from <a href='https://en.wikipedia.org/wiki/GNU_bison'>Wikipedia</a>:
 # Makefile
 
 FILES	= Lexer.c Parser.c Expression.c main.c
 CC	= g++
 CFLAGS	= -g -ansi
 
 test:		$(FILES)
 		$(CC) $(CFLAGS) $(FILES) -o test
 
 Lexer.c:	Lexer.l 
 		flex Lexer.l
 
 Parser.c:	Parser.y Lexer.c
 		bison Parser.y
 
 clean:
 		rm -f *.o *~ Lexer.c Lexer.h Parser.c Parser.h test

