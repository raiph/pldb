 #define FC_HIDDEN          2
 #define FC_SYSTEM          4
 
 // New-style comment
 #command SET DELETED &lt;x:ON,OFF,&amp;&gt;      =&gt; Set( _SET_DELETED, &lt;(x)&gt; )
