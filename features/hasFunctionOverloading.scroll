codeWithHeader Example from <a href="../concepts/cpp.html">C++</a>:
 // volume of a cube
 int volume(const int s) {
  return s*s*s;
 }
 // volume of a cylinder
 double volume(const double r, const int h) {
   return 3.1415926*r*r*static_cast&lt;double&gt;(h);
 }

codeWithHeader Example from <a href="../concepts/pascal.html">Pascal</a>:
 program Adhoc;
 
 function Add(x, y : Integer) : Integer;
 begin
     Add := x + y
 end;
 
 function Add(s, t : String) : String;
 begin
     Add := Concat(s, t)
 end;
 
 begin
     Writeln(Add(1, 2));                   (* Prints &quot;3&quot;             *)
     Writeln(Add(&#39;Hello, &#39;, &#39;World!&#39;));    (* Prints &quot;Hello, World!&quot; *)
 end.

codeWithHeader Example from <a href="../concepts/speedie.html">Speedie</a>:
 function volume (|float| a, |float|)
  return a*a*a
 function volume (|float| r, |float| h, |float|)
  // volume of a cylinder
  return math.pi*r*r*h

